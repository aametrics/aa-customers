{
    "name": "aa-customers/wordpress-plugin",
    "description": "AA Customers - Membership management, event sales, and practitioner directory for WordPress",
    "type": "wordpress-plugin",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "dompdf/dompdf": "^2.0",
        "stripe/stripe-php": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "classmap": [
            "core/",
            "admin/"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
